{
	"name": "wantosing-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.3.0",
		"daisyui": "^5.1.27",
		"next": "15.4.6",
		"qrcode.react": "^4.2.0",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4.1.14",
		"@types/node": "^20.19.19",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"patch-package": "^8.0.1",
		"tailwindcss": "^4.1.14",
		"typescript": "^5",
		"wrangler": "^4.42.0"
	},
	"postinstall": "node -e \"if (process.env.NODE_ENV !== 'production' && process.env.CI !== 'true') { require('child_process').execSync('patch-package', {stdio: 'inherit'}) }\"",
	"browserslist": "> 1%"
}
